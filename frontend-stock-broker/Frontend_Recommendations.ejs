    <style>
        * {
            box-sizing: border-box;
          }
          .center{
            text-align: center;
          }

          .column {
            border-style:solid;
            border-color: white;
            text-align: center;
            column-gap: 30px;
            float: left;
            width: 33.33%;
            padding: 20px;
            height: 460px;
            box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19);
          }
          .border {
            border-style:solid;
            border-color: white;
          }
          .column1 {
            border-style:solid;
            border-color: white;
            text-align: center;
            float: left;
            width: 25%;
            padding: 10px;
            height: 650px;
            box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19);
          }

          .column2 {
            border-style:solid;
            border-color: white;
            text-align: center;
            color: white;
            float: left;
            width: 20%;
            padding: 10px;
            height: 250px;
            box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19);
          }

          .row:after {
            content: "";
            display: table;
            clear: both;
          }
          .graph {
            border-style:solid;
            border-color: gray;
            
          }
          .image {
            display: block;
            margin-left: auto;
            margin-right: auto;
          }
          .graph{
            border-style:solid;
            border-color: gray;
          }
          .btn-group button {
            
            border: 2px solid white; 
            color: white;
            padding: 10px 24px;
            cursor: pointer;
            float: left;
          }

    </style>
</head>
